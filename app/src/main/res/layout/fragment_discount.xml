<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_42sdp"
        android:orientation="horizontal" />

    <TextView
        android:id="@+id/tv_napthe_dt"
        style="@style/basetext"
        android:layout_width="@dimen/_65sdp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_margin="@dimen/_2sdp"
        android:gravity="center"
        android:text="Nạp thẻ ĐT"
        android:textAllCaps="true"
        android:textStyle="bold" />

    <View
        android:id="@+id/view_horizontal_1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/tv_napthe_dt"
        android:background="@color/colorPrimary" />

    <TextView
        android:id="@+id/tv_muamathe"
        style="@style/basetext"
        android:layout_width="@dimen/_65sdp"
        android:layout_margin="@dimen/_2sdp"
        android:layout_toLeftOf="@+id/tv_napthe_dt"
        android:gravity="center"
        android:text="mua mã thẻ"
        android:textAllCaps="true"
        android:textStyle="bold" />

    <View
        android:id="@+id/view_vertical_2"
        android:layout_width="1dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/view_horizontal_2"
        android:layout_toRightOf="@+id/tv_muamathe"
        android:background="@color/colorPrimary" />

    <View
        android:id="@+id/view_vertical_1"
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:layout_toLeftOf="@+id/tv_muamathe"
        android:background="@color/colorPrimary" />

    <View
        android:id="@+id/view_horizontal_2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1sdp"
        android:layout_below="@+id/ic_vietnammobile"
        android:background="@color/colorPrimary" />

    <View
        android:id="@+id/view_horizontal_3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1sdp"
        android:layout_below="@id/tv_napthegame"
        android:background="@color/colorPrimary" />

    <TextView
        android:id="@+id/tv_napthegame"
        style="@style/basetext"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view_horizontal_2"
        android:layout_margin="@dimen/_10sdp"
        android:layout_toRightOf="@+id/view_vertical_1"
        android:gravity="center"
        android:text="nạp thẻ game"
        android:textAllCaps="true"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/ic_vinafone"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/view_horizontal_1"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_toLeftOf="@id/view_vertical_1"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_vinaphone" />

    <TextView
        android:id="@+id/tv_num_vina_muama_the"
        style="@style/numbertext"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/view_horizontal_1"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toLeftOf="@id/view_vertical_2"
        android:layout_toRightOf="@+id/view_vertical_1"
        android:gravity="center"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tv_num_vina_napthe_dt"
        style="@style/numbertext"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/view_horizontal_1"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toRightOf="@+id/view_vertical_2"
        android:gravity="center"
        android:text="4%"
        android:textSize="18sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/ic_mobifone"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/ic_vinafone"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toLeftOf="@id/view_vertical_1"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_mobifone" />

    <TextView
        android:id="@+id/tv_num_mobi_muama_the"
        style="@style/numbertext"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/tv_num_vina_muama_the"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toLeftOf="@id/view_vertical_2"
        android:layout_toRightOf="@+id/view_vertical_1"
        android:gravity="center"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tv_num_mobi_napthe_dt"
        style="@style/numbertext"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/tv_num_vina_napthe_dt"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toRightOf="@+id/view_vertical_2"
        android:gravity="center"
        android:text="4%"
        android:textSize="18sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/ic_viettel"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/ic_mobifone"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toLeftOf="@id/view_vertical_1"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_viettel" />

    <TextView
        android:id="@+id/tv_num_viettel_muama_the"
        style="@style/numbertext"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/tv_num_mobi_muama_the"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toLeftOf="@id/view_vertical_2"
        android:layout_toRightOf="@+id/view_vertical_1"
        android:gravity="center"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tv_num_viettel_napthe_dt"
        style="@style/numbertext"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/tv_num_mobi_napthe_dt"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toRightOf="@+id/view_vertical_2"
        android:gravity="center"
        android:text="1%"
        android:textSize="18sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/ic_vietnammobile"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/ic_viettel"
        android:layout_marginBottom="@dimen/xsmall_radius_5dp"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toLeftOf="@id/view_vertical_1"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_vietnammobile" />

    <TextView
        android:id="@+id/tv_num_vietnammobi_muama_the"
        style="@style/numbertext"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/tv_num_viettel_muama_the"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toLeftOf="@id/view_vertical_2"
        android:layout_toRightOf="@+id/view_vertical_1"
        android:gravity="center"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tv_num_vietnammobi_napthe_dt"
        style="@style/numbertext"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/tv_num_viettel_napthe_dt"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toRightOf="@+id/view_vertical_2"
        android:gravity="center"
        android:text="-%"
        android:textSize="18sp"
        android:textStyle="bold" />

    <!-- <ImageView android:id="@+id/ic_gmobile" android:layout_width="match_parent" android:layout_height="@dimen/height_button_primary" android:layout_below="@+id/ic_vietnammobile" android:layout_marginTop="@dimen/small_radius" android:layout_toLeftOf="@id/view_vertical_1" android:scaleType="fitCenter" android:src="@drawable/ic_gmobile" /> <TextView android:id="@+id/tv_num_gmobile_muama_the" style="@style/numbertext" android:layout_width="match_parent" android:layout_height="@dimen/height_button_primary" android:layout_below="@+id/tv_num_vietnammobi_muama_the" android:layout_marginTop="@dimen/small_radius" android:layout_toLeftOf="@id/view_vertical_2" android:layout_toRightOf="@+id/view_vertical_1" android:gravity="center" android:text="-%" android:textStyle="bold" /> <TextView android:id="@+id/tv_num_gmobile_napthe_dt" style="@style/numbertext" android:layout_width="match_parent" android:layout_height="@dimen/height_button_primary" android:layout_below="@+id/tv_num_vietnammobi_napthe_dt" android:layout_marginTop="@dimen/small_radius" android:layout_toRightOf="@+id/view_vertical_2" android:gravity="center" android:text="-%" android:textStyle="bold" />-->


    <ImageView
