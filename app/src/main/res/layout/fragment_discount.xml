    <ImageView
        android:id="@+id/ic_garena"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/ic_fptgate"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toLeftOf="@id/view_vertical_1"
        android:scaleType="fitCenter"
