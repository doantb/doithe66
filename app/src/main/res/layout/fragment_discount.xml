        android:src="@drawable/ic_fptgate" />

    <TextView
        android:id="@+id/tv_discount_fptgate"
        style="@style/numbertext"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_ic"
        android:layout_below="@+id/tv_discount_zing"
        android:layout_marginTop="@dimen/xsmall_radius_5dp"
        android:layout_toRightOf="@id/view_vertical_1"
        android:gravity="center"
        android:textSize="18sp"
        android:textStyle="bold" />

